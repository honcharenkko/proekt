        rows = cursor.fetchall()
        text = "üì¶–í–ê–®–ï –ó–ê–ú–û–í–õ–ï–ù–ù–Øüì¶\n"
        for row in rows:
            if row[7] == 0:
                st = "–í–ò–ö–û–ù–£–Ñ–¢–¨–°–Ø"
                text += f"üìå–ù–æ–º–µ—Ä –∑–∞–º–æ–≤–ª–µ–Ω–Ω—è: {row[0]}\nüíµ–¶—ñ–Ω–∞: {row[4]}\n‚öñ–í–∞–≥–∞:{row[5]}\nüó∫Ô∏è–ê–¥—Ä–µ—Å–∞: {row[9]}, {row[10]} {row[11]}\nüì™–°—Ç–∞—Ç—É—Å: {st}\n\n"
            if row[7] == 1:
                st = "–ì–û–¢–û–í–û"
                text += f"üìå–ù–æ–º–µ—Ä –∑–∞–º–æ–≤–ª–µ–Ω–Ω—è: {row[0]}\nüíµ–¶—ñ–Ω–∞: {row[4]}\n‚öñ–í–∞–≥–∞:{row[5]}\nüó∫Ô∏è–ê–¥—Ä–µ—Å–∞: {row[9]}, {row[10]} {row[11]}\nüì´–°—Ç–∞—Ç—É—Å: {st}\n\n"
        bot.send_message(message.chat.id, text)
