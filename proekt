        keyboard = types.InlineKeyboardMarkup();  # –Ω–∞—à–∞ –∫–ª–∞–≤—ñ–∞—Ç—É—Ä–∞
        key_it1 = types.InlineKeyboardButton(text='IPHONE', callback_data='iphone');  # –∫–Ω–æ–ø–∫–∞ –í–∏–∫–ª–∏–∫—É —Ç–æ–≤–∞—Ä—É
