        qwerty = f"SELECT order_id, SUM(Total_price), SUM(Total_weight), Status FROM ORDERR WHERE id_customer == {people_id} AND order_id = {random_numberss}"
        cursor.execute(qwerty)
        rows = cursor.fetchall()
        text = "üì¶–í–ê–®–ï –ó–ê–ú–û–í–õ–ï–ù–ù–Øüì¶\n"

        for row in rows:
            if row[3] == 0:
                st = "–í–ò–ö–û–ù–£–Ñ–¢–¨–°–Ø"
                text += f"üìå–ù–æ–º–µ—Ä –∑–∞–º–æ–≤–ª–µ–Ω–Ω—è: {row[0]}\nüíµ–¶—ñ–Ω–∞: {row[1]}\n‚öñ–í–∞–≥–∞:{row[2]}\nüì™–°—Ç–∞—Ç—É—Å: {st}"
        bot.send_message(call.message.chat.id, text)
bot.polling(none_stop=True)
